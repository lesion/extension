{
   "background": {
      "scripts": [ "scripts/chromereload.js", "scripts/vendor/lodash.js", "scripts/vendor/sha256.js", "scripts/vendor/peer.js", "scripts/vendor/webrtc-chord.js", "scripts/browserified/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "images/icon-19.png",
         "38": "images/icon-38.png"
      },
      "default_popup": "popup.html",
      "default_title": "Scholar Ninja"
   },
   "content_scripts": [ {
      "css": [ "styles/content.css" ],
      "js": [ "scripts/vendor/jquery.js", "scripts/vendor/unslider.js", "scripts/browserified/content.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "default_locale": "en",
   "description": "An open distributed search engine for science",
   "icons": {
      "128": "images/icon-128.png",
      "16": "images/icon-16.png",
      "48": "images/icon-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Scholar Ninja",
   "permissions": [ "unlimitedStorage", "storage", "tabs", "http://*/*", "https://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.19",
   "web_accessible_resources": [ "images/*.png", "images/*.svg" ]
}
